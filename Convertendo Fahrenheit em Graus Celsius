package exercicios;

import java.util.Scanner;

public class Exer12 {

	public static void main(String[] args) {
		Scanner geo= new Scanner(System.in);
		float f;
		System.out.print("Digite a temperatura em fahrenheit:");
		f = geo.nextFloat();
		
		float c =( 5 * (f - 32) / 9 );
		
		System.out.print("A temperatura em graus celsius:"+c);
		geo.close();

	}

}
